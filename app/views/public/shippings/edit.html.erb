
      <h4>配送先編集</h4>

     <!-- form_with部分 -->
    <%= form_with model: @shipping, url: shipping_path(@shipping), local: true do |f| %>

          <label>郵便番号(ハイフンなし)</label>
        <div>
          <%= f.text_field :post_cod %>
        </div>
   

          <label>住所</label>
        <div>
          <%= f.text_field :address %>
        </div>
 
         <label>宛名</label>
        <div>
          <%= f.text_field :name %>
        </div>
        <div>
          <%= f.submit '変更を保存', class:'btn btn-success' %>
        </div>

  <% end %>